{
  "name": "unit",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:samuelmtimbo/unit.git",
  "author": "Samuel Timb√≥ <samuelmtimbo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch:server": "NODE_ENV=development nodemon --config nodemon.json",
    "watch:bundle": "ts-node src/script/watch/client.ts",
    "watch:client": "run-p watch:bundle watch:worker",
    "watch:worker": "ts-node src/script/watch/worker.ts",
    "watch:worker-solc": "ts-node src/script/watch/worker-solc.ts",
    "watch": "run-p watch:server watch:client",
    "start": "node lib/server/index.js",
    "setup": "yarn; run-s build lib",
    "prepare": "run-s build lib",
    "tcheck": "tsc --noEmit",
    "lib": "rm -rf lib; tsc",
    "test": "ts-node src/test/index.ts",
    "build": "run-s build:system build:client",
    "build:bundle": "ts-node src/script/build/client.ts",
    "build:client": "run-p build:bundle build:worker",
    "build:system": "ts-node src/script/build/system.ts",
    "build:worker": "ts-node src/script/build/worker.ts",
    "build:worker-solc": "ts-node src/script/build/worker-solc.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "evaluatex": "2.2.0",
    "eventemitter2": "5.0.1",
    "express": "4.17.1",
    "fuzzy": "0.1.3",
    "glob": "7.1.6",
    "http-errors": "1.8.0",
    "is-ip": "3.1.0",
    "querystring": "0.2.0",
    "resize-observer-polyfill": "1.5.1"
  },
  "devDependencies": {
    "@types/eventemitter2": "4.1.0",
    "@types/json-diff": "0.5.1",
    "@types/node": "13.11.1",
    "@types/web-bluetooth": "0.0.8",
    "@types/websocket": "1.0.1",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "esbuild": "0.12.21",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "fs-extra": "10.0.0",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.2",
    "npm-run-scripts": "1.4.4",
    "prettier": "2.3.1",
    "ts-node": "10.0.0",
    "typescript": "4.4.3"
  }
}
